{
  "expo": {
    "name": "Citizenx",
    "slug": "citixenx",
    "version": "1.5.5",
    "orientation": "portrait",
    "icon": "./assets/images/citizenx.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images//citizenx.png",
      "resizeMode": "contain",
      "backgroundColor": "#f3f2f2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSPhotoLibraryUsageDescription": "Citizen X needs access to your photos to upload a profile picture."
      },
      "bundleIdentifier": "com.kennedy757.citixenx"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/citizenx.png",
        "backgroundColor": "#0E9C67",
        "targetSdkVersion": 34
      },
      "package": "com.kennedy757.citixenx",
      "versionCode": 68,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/citizenx.png"
    },
    "scheme": "citizenx",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow CITIZEN_X to access your camera",
          "microphonePermission": "Allow CITIZEN_X to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow CITIZEN_X to access your photos.",
          "savePhotosPermission": "Allow CITIZEN_X to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow CITIZEN_X to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Citizen X to use your Photo."
        }
      ],
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "06525c8d-e5e0-450e-93ec-ce6da43f75da"
      }
    }
  }
}
